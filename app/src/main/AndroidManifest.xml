<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.brauma.withinyourmeans">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_my_own_launcher_second"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_my_own_launcher_second_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Activity.AddCategoryActivity"></activity>
        <activity
            android:name=".Activity.AddExpenseActivity"
            android:label="Add Expense"></activity>
        <activity android:name=".Activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>